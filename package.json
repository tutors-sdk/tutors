{
  "name": "tutors",
  "repository": "https://github.com/tutors-sdk/tutors.git",
  "author": "Eamonn De Leastar <eamonn.deleastar@setu.ie>",
  "license": "MIT",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "cli/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "course": "turbo run dev --filter tutors-course",
    "home": "turbo run dev --filter tutors-home",
    "time": "turbo run dev --filter tutors-time",
    "lint": "turbo run lint",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-custom": "*",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "turbo": "latest"
  },
  "packageManager": "npm@8.19.2"
}
